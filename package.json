{
  "name": "webcreeper",
  "version": "0.0.48",
  "description": "WebCreeper easy web crawler",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "deploy": "npm run build && npm run test:cov && npm publish --access public",
    "test": "jest --passWithNoTests",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremiassantos/webcreeper.git"
  },
  "keywords": [
    "crawler",
    "scraper",
    "crawling"
  ],
  "author": "Jeremias Silva (santos.jeremias952@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeremiassantos/webcreeper/issues"
  },
  "homepage": "https://github.com/jeremiassantos/webcreeper#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^6.0.46",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^8.3.0",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "request": "2.83.0",
    "request-promise": "^4.2.2",
    "cheerio": "1.0.0-rc.10",
    "iconv-lite": "^0.4.24",
    "replaceall": "^0.1.6",
    "request-cookies": "^1.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".(spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
